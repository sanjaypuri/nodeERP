SELECT
	*, invvalue-advadjusted-retadjusted+addcharges receivable, null received
FROM
(
	SELECT
		br.invdate date, br.invno invrefno, br.description, br.bgid, br.remarks, br.basicamount,
		iif(br.description = 'Against Supplies', br.basicamount, 0) billedagainstsupplies, 
		iif(br.description = 'Against Supplies', br.basicamount+br.taxes, br.basicamount) invvalue,
		iif(br.description = 'Against Supplies', br.basicamount*cp.totaladvances/cp.basicpovalue , 0) advadjusted,
		iif(br.description = 'Against Supplies', br.basicamount*cp.totalretention/cp.basicpovalue , 0) retadjusted,
		ifnull(br.addcharges,0) addcharges,
		iif(br.description = 'Against Supplies', br.basicamount+br.taxes, 0) receivableagainstsupplies
	FROM billsraised br
	LEFT JOIN customerpo cp on cp.projectno =  br.projectno
	WHERE br.projectno = '22204'
	ORDER BY br.invdate
)

UNION ALL

SELECT
	datereceived date, RefInvoice invrefno, Description description, null bgid, Remarks remarks, 
	null basicamount, null billedagainstsupplies, null invvalue, null advadjusted, null retadjusted,
	null addcharges, null receivableagainstsupplies,
	null receivable,
	Amount received
FROM paymentsreceived
WHERE projectno = '22204'

ORDER BY date</sql><sql name="SQL 3">-- CREATE VIEW projectpaymentdetails as 
